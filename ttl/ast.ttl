@prefix : <http://data.treehouse.example/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://data.treehouse.example/> .

<http://data.treehouse.example/> rdf:type owl:Ontology ;
                                  rdfs:comment "A simple ontology for astronomical concepts, used for demonstrational purposes.e"@en ,
                                               "En enkel ontologi for astronomiske konsepter, brukt i demonstasjons√∏yemed."@nb ;
                                  rdfs:label "Astronomi"@nb ,
                                             "Astronomy lite"@en .

#################################################################
#    Object Properties
#################################################################

###  http://data.treehouse.example/memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:domain :AstronomicalBody ;
          rdfs:range :PlanetarySystem ;
          rdfs:comment "An astronomical body can be member of a planetary system."@en ;
          rdfs:label "member of"@en .


###  http://data.treehouse.example/orbits
:orbits rdf:type owl:ObjectProperty ;
        rdfs:domain :NaturalSatellite ;
        rdfs:range :Planet ;
        rdfs:label "orbits" .


#################################################################
#    Data properties
#################################################################

###  http://data.treehouse.example/albedo
:albedo rdf:type owl:DatatypeProperty ;
        rdfs:domain :NaturalSatellite ;
        rdfs:range xsd:long ;
        rdfs:label "albedo"@en .


###  http://data.treehouse.example/lengthOfDay
:lengthOfDay rdf:type owl:DatatypeProperty ;
             rdfs:domain :Planet ;
             rdfs:range xsd:double ;
             rdfs:label "length of day"@en .


###  http://data.treehouse.example/meanTemperature
:meanTemperature rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Planet ;
                 rdfs:range xsd:long ;
                 rdfs:label "mean temperature"@en .


###  http://data.treehouse.example/orbitalPeriod
:orbitalPeriod rdf:type owl:DatatypeProperty ;
               rdfs:domain :Planet ;
               rdfs:range xsd:double ;
               rdfs:label "orbital period"@en .


###  http://data.treehouse.example/radius
:radius rdf:type owl:DatatypeProperty ;
        rdfs:domain :NaturalSatellite ;
        rdfs:range xsd:double ;
        rdfs:label "radius"@en .


#################################################################
#    Classes
#################################################################

###  http://data.treehouse.example/AstronomicalBody
:AstronomicalBody rdf:type owl:Class ;
                  rdfs:label "Astronomical Body"@en .


###  http://data.treehouse.example/AstronomicalObject
:AstronomicalObject rdf:type owl:Class ;
                    rdfs:label "Astronomical Object"@nb .


###  http://data.treehouse.example/FrozenWorld
:FrozenWorld rdf:type owl:Class ;
             rdfs:subClassOf :PlanetaryClasses ;
             rdfs:label "Frozen world"@en .


###  http://data.treehouse.example/HotWorld
:HotWorld rdf:type owl:Class ;
          rdfs:subClassOf :PlanetaryClasses ;
          rdfs:label "Hot world"@en .


###  http://data.treehouse.example/NaturalSatellite
:NaturalSatellite rdf:type owl:Class ;
                  rdfs:subClassOf :AstronomicalBody ;
                  rdfs:label "Natural satellite"@en .


###  http://data.treehouse.example/Planet
:Planet rdf:type owl:Class ;
        rdfs:subClassOf :AstronomicalBody ;
        rdfs:label "Planet"@en .


###  http://data.treehouse.example/PlanetaryClasses
:PlanetaryClasses rdf:type owl:Class ;
                  rdfs:label "Planetary categories"@en .


###  http://data.treehouse.example/PlanetarySystem
:PlanetarySystem rdf:type owl:Class ;
                 rdfs:subClassOf :AstronomicalObject ;
                 rdfs:label "Planetary system"@en .


###  http://data.treehouse.example/Star
:Star rdf:type owl:Class ;
      rdfs:subClassOf :AstronomicalBody ;
      rdfs:label "Star"@en .


###  http://data.treehouse.example/StarCluster
:StarCluster rdf:type owl:Class ;
             rdfs:subClassOf :AstronomicalObject ;
             rdfs:label "Star cluster"@en .


###  http://data.treehouse.example/TemperateWorld
:TemperateWorld rdf:type owl:Class ;
                rdfs:subClassOf :PlanetaryClasses ;
                rdfs:label "Temperate world"@en .


